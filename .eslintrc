{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "rallycoding"
  ],
  "rules": {
    "no-mixed-spaces-and-tabs": 1,
    "prefer-template": 0,
    "object-shorthand": 0,
    "no-underscore-dangle": 0,
    "react/jsx-equals-spacing": 0,
    "no-else-return": 0,
    "import/no-named-as-default": 0,
    "arrow-body-style": 0,
    }
}
